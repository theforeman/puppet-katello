{
  "name": "katello-katello",
  "version": "6.0.0",
  "author": "Katello",
  "summary": "Handles deploying and managing a Katello + Foreman server",
  "license": "GPL-3.0+",
  "source": "https://github.com/Katello/puppet-katello.git",
