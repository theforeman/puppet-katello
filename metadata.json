    },
    {
      "name": "katello/certs",
      "version_requirement": ">= 11.0.0 < 13.0.0"
    },
    {
      "name": "katello/qpid",
      "version_requirement": ">= 8.0.0 < 9.0.0"
    },
    {
      "name": "theforeman/foreman",
      "version_requirement": ">= 17.0.0 < 18.0.0"
    }
