  "dependencies": [
    {
      "name": "puppet/extlib",
      "version_requirement": ">= 3.0.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 1.0.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
