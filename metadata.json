{
  "name": "katello-katello",
  "version": "25.1.0",
  "author": "katello",
  "summary": "Handles deploying and managing a Katello + Foreman server",
  "license": "GPL-3.0+",
  "source": "https://github.com/theforeman/puppet-katello.git",
  "project_page": "https://github.com/theforeman/puppet-katello",
